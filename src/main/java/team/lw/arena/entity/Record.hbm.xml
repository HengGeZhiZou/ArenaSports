<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.lw.arena.entity.Record" table="record" schema="project_db">
        <id name="count" column="_count"/>
        <property name="id" column="_id"/>
        <property name="date" column="_date"/>
        <property name="position" column="_position"/>
        <property name="type" column="_type"/>
    </class>
</hibernate-mapping>