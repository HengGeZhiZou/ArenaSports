<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.lw.arena.entity.UserInfo" table="user_info" schema="project_db">
        <id name="id" column="_id"/>
        <property name="name" column="_name"/>
        <property name="portrait" column="_portrait"/>
        <property name="sex" column="_sex"/>
        <property name="age" column="_age"/>
        <property name="height" column="_height"/>
        <property name="weight" column="_weight"/>
        <property name="profiles" column="_profiles"/>
        <property name="phone" column="_phone"/>
        <property name="playing" column="playing" />
        <property name="addTime" column="add_time"/>
    </class>
</hibernate-mapping>